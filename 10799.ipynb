   "source": [
    "inputs = list(input())\n",
    "sum = 0\n",
    "st = []\n",
    "\n",
    "for i in range(len(inputs)):\n",
    "    if inputs[i] == '(':\n",
    "        st.append('(')\n",
    "    \n",
    "    else:\n",
    "        if inputs[i-1] =='(':\n",
    "            st.pop()\n",
    "            sum += len(st)\n",
    "        else:\n",
    "            st.pop()\n",
    "            sum += 1\n",
    "    \n",
    "print(sum)"
   ]
  }
